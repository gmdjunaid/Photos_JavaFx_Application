            <Button fx:id="leftButton" layoutX="3.0" layoutY="124.0" mnemonicParsing="false" onAction="#handleLeftButton" prefHeight="63.0" prefWidth="27.0" text="&lt;" />
            <Button fx:id="rightButton" layoutX="609.0" layoutY="124.0" mnemonicParsing="false" onAction="#handleRightButton" prefHeight="63.0" prefWidth="27.0" text="&gt;" />
